import java.util.ArrayList;
public class Solver {
    private double num1;
    private double num2;
    private ArrayList<Memory> one;
    public Solver()
    {
       one = new ArrayList<Memory>();
    }
    public double sum(double num1, double num2)
    {
        return num1 + num2;
    }
    public double diffrence(double num1, double num2)
    {
        return num1 -num2;
    }
    public double Product(double num1, double num2)
    {
        return num1*num2;
    }
    public double quotient(double num1, double num2)
    {
        return num1/num2;
    }
    public double exponent(double num1, double num2)
    {
        return  Math.pow(num1, num2);
    }
    public double Roots(double num1, double num2)
    {
        return Math.pow(num1, (1/num2));
    }
    public double naturalLog(double num1)
    {
        return Math.pow(2.71828, num1);
    }
}
